set term png linewidth 3
set output sprintf("%04d.png", s)
set key off
plot [0:][0:0.8] \
     "<xargs < data/alpha -n1 ./rnd2 0" w lp, "data/l.0" w l, \
     "<xargs < data/alpha -n1 ./rnd2 0.3" w lp, "data/l.0.3" w l, \
     "<xargs < data/alpha -n1 ./rnd2 0.8" w lp, "data/l.0.8" w l, \
     "<xargs < data/alpha -n1 ./rnd2 1" w lp, "data/l.1" w l
